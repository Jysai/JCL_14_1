{
  "name": "@jysai/datepicker",
  "version": "0.1.4",
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "del dist",
    "prepare-build": "npm run clean && NODE_ENV=production babel lib --out-dir dist --extensions \".js\" --ignore 'lib/**/*.text.js'",
    "copy": "copy README.md",
    "build": "npm run prepare-build && npm run copy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "directories": {
    "lib": "lib"
  },
  "author": "Jysai",
  "license": "ISC",
  "description": "A date picker in the form of a calendar for a reactJs component"
}
